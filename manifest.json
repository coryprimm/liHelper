{
  "name": "liHelper",
  "version": "0.1",
  "description": "Removes already-applieds automatically. Version 1, only activate when visiting li",
  "permissions": [
    "activeTab", "scripting", "tabs"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/jobs", "https://www.linkedin.com/jobs*", "https://www.linkedin.com/jobs/*", "https://www.linkedin.com/jobs//*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 3,
   "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}

