{
  "name": "liHelper",
  "version": "0.1",
  "description": "Removes already-applieds automatically",
  "permissions": [
    "activeTab", "scripting", "tabs"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/jobs", "https://www.linkedin.com/jobs*", "https://www.linkedin.com/jobs/*", "https://www.linkedin.com/jobs//*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 3,
   "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}

